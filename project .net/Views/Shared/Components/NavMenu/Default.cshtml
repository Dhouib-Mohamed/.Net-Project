@model Dictionary<string, string>




<nav class="navbar fixed-top  navbar-expand-sm  navbar-light bg-white border-bottom box-shadow mb-3">
                <div class="container-fluid">
                <div> 
                    <img src="https://i.pinimg.com/564x/2a/c5/60/2ac560c628f1577e6db6ade2117ce828.jpg" style="height:35px" />
                <a class="navbar-brand" asp-area="" asp-controller="Restaurant" asp-action="index" style="text-decoration:none;color:black">MyRestaurant.net</a>
        </div>
                
        <div class=" row">
            @foreach (var link in Model)
         {
             if (link.Key == "search")
                    {
                    <form class="d-flex  w-auto" action="/Restaurant/RestaurantsByName" method="post">
                    <input class="form-control me-sm-2" name="search" type="search" placeholder="Search">
                    <button class="btn btn-secondary my-2 my-sm-0" type="submit">Search</button>
                    </form>
                    }
                    if (link.Key == "Sign up" || link.Key == "Sign in")
                    {

                <a class="nav-link text-dark w-auto m-1" href="@link.Value" style="text-decoration:none;color:black"><button class="btn btn-dark">@link.Key</button></a>

                     
                    }
                    if(link.Key == "profile")
                    {

                    <a href="@link.Value" class="w-auto">
                        <svg xmlns="http://www.w3.org/2000/svg" width="28" height="28" fill="black" class="bi bi-person-circle " viewBox="0 0 16 16">
  <path d="M11 6a3 3 0 1 1-6 0 3 3 0 0 1 6 0z"/>
  <path fill-rule="evenodd" d="M0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8zm8-7a7 7 0 0 0-5.468 11.37C3.242 11.226 4.805 10 8 10s4.757 1.225 5.468 2.37A7 7 0 0 0 8 1z"/>
</svg></a>
                   
                        
                    }
                    if (link.Key == "signout")
                    {

                    <a  href=" @link.Value" class="w-auto">
                          <svg xmlns="http://www.w3.org/2000/svg" width="28" height="28" fill="black" class="bi bi-box-arrow-right " viewBox="0 0 16 16">
                            <path fill-rule="evenodd" d="M10 12.5a.5.5 0 0 1-.5.5h-8a.5.5 0 0 1-.5-.5v-9a.5.5 0 0 1 .5-.5h8a.5.5 0 0 1 .5.5v2a.5.5 0 0 0 1 0v-2A1.5 1.5 0 0 0 9.5 2h-8A1.5 1.5 0 0 0 0 3.5v9A1.5 1.5 0 0 0 1.5 14h8a1.5 1.5 0 0 0 1.5-1.5v-2a.5.5 0 0 0-1 0v2z"/>
                            <path fill-rule="evenodd" d="M15.854 8.354a.5.5 0 0 0 0-.708l-3-3a.5.5 0 0 0-.708.708L14.293 7.5H5.5a.5.5 0 0 0 0 1h8.793l-2.147 2.146a.5.5 0 0 0 .708.708l3-3z"/>
                        </svg>
                     </a>
                       
                   
                    }
                }
            
        </div>
